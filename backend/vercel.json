{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "../package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "../build"
            }
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "app.py" },
        { "src": "/(.*)", "dest": "../build/$1" }
    ]
}
